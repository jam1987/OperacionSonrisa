$(document).ready(function(){
    $.ajaxPrefilter(function(options, originalOptions, jqXHR) {
        if(!options.crossDomain) {
            if(options.data) {
                options.data += "&";
            } else {
                options.data = "";
            }
            options.data += "csrfmiddlewaretoken={{csrf_token}}";
        }
    });
})



</script>


